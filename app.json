{
  "expo": {
    "name": "Take Flight",
    "slug": "take-flight",
    "scheme": "take-flight",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "server",
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.takeflight.app"
    },
    "plugins": [
      ["expo-router"],
      ["@react-native-google-signin/google-signin"]
    ]
  }
}
